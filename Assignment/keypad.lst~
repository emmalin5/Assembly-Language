keypad                                                                                                                  PAGE 1
                         1     ORG 0000H            ; Start at address 0
                         2
                         3     MAIN:
0000 7590F0              4         MOV P1, #0F0H    ; Set upper 4 bits (columns) as input and lower 4 bits (rows) as output
                         5     SCAN:
0003 74FE                6         MOV A, #0FEH     ; Start with row 1 low (others high)
0005 1115                7         ACALL CHECKCOL   ; Check if any key in this row is pressed
                         8
0007 74FD                9         MOV A, #0FDH     ; Set row 2 low (others high)
0009 1115               10         ACALL CHECKCOL   ; Check if any key in this row is pressed
                        11
000B 74FB               12         MOV A, #0FBH     ; Set row 3 low (others high)
000D 1115               13         ACALL CHECKCOL   ; Check if any key in this row is pressed
                        14
000F 74F7               15         MOV A, #0F7H     ; Set row 4 low (others high)
0011 1115               16         ACALL CHECKCOL   ; Check if any key in this row is pressed
                        17
0013 80EE               18         SJMP SCAN        ; Repeat scanning
                        19
                        20     CHECKCOL:
0015 F590               21         MOV P1, A        ; Output current row pattern to P1
0017 00                 22         NOP              ; Small delay for settling
0018 00                 23         NOP
0019 E590               24         MOV A, P1        ; Read columns
                        25
001B 54F0               26         ANL A, #0F0H     ; Mask the upper 4 bits (columns)
001D B4F001             27         CJNE A, #0F0H, DECODEKEY  ; If any column is low, a key is pressed
                        28
0020 22                 29         RET              ; No key pressed, return
                        30
                        31     DECODEKEY:
                        32         ; Here you would add logic to decode which key was pressed
                        33         ; based on the row and column combination
                        34         ; For example, if Row 1 (P1.0 low) and Column 1 (P1.4 low),
                        35         ; that would correspond to key 1 on a typical keypad.
                        36
                        37         ; For simplicity, we'll just flash an LED or something to indicate a key press.
0021 75A0FF             38         MOV P2, #0FFH    ; Turn on LEDs on Port 2 (for example)
0024 112C               39         ACALL DELAY      ; Simple delay
0026 75A000             40         MOV P2, #00H     ; Turn off LEDs
0029 112C               41         ACALL DELAY
                        42
002B 22                 43         RET              ; Return to continue scanning
                        44
                        45     DELAY:
002C 79FF               46         MOV R1, #255     ; Outer loop
                        47
                        48     DELAY_LOOP:
002E D9FE               49         DJNZ R1, DELAY_LOOP ; Inner loop countdown
                        50
0030 22                 51         RET
                        52
                        53     END
